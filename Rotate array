#include<bits/stdc++.h>
using namespace std;
void rotateArr(int arr[], int d, int n){
    // code here
    int i,a[n];
    for(i=0;i<n;i++){
        a[i]=arr[(i+d)%n];
       }
    for(i=0;i<n;i++){
        arr[i]=a[i];
       }   
       
}

int main() {
	int t;
	cin >> t;
	
	while(t--){
	    int n, d;
	    cin >> n >> d;
	    
	    int arr[n];
	    
	    for(int i = 0; i < n; i++){
	        cin >> arr[i];
	    }
	    
	    rotateArr(arr, d,n);
	    
	    for(int i =0;i<n;i++){
	        cout << arr[i] << " ";
	    }
	    cout << endl;
	}
	return 0;
}  
